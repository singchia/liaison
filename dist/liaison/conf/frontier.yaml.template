daemon:
  pprof:
    addr: 127.0.0.1:6061
    enable: true
edgebound:
  listen:
    network: tcp
    addr: 0.0.0.0:${FRONTIER_PORT}
    tls:
      enable: true
      insecure_skip_verify: true
      certs:
      - cert: /opt/liaison/certs/server.crt
        key: /opt/liaison/certs/server.key
  edgeid_alloc_when_no_idservice_on: false
servicebound:
  listen:
    network: tcp
    addr: 127.0.0.1:30011